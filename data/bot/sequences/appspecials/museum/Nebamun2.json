    "Steps": [
        {
            "Type": "Text",
            "Label": {
                "en": "The paintings are interesting because they tell us how Nebamun wanted himself to be viewed...",
                "fr": "The paintings are interesting because they tell us how Nebamun wanted himself to be viewed...",
                "es": "The paintings are interesting because they tell us how Nebamun wanted himself to be viewed..."
            }
        },
        {
            "Type": "Text",
            "Label": {
                "en": "It's important to remember that only the rich, scribes and priests, could read",
                "fr": "It's important to remember that only the rich, scribes and priests, could read",
                "es": "It's important to remember that only the rich, scribes and priests, could read"
            }
        },
        {
            "Type": "Text",
            "Label": {
                "en": "And so Nebamun is bragging to other elites how fantastic his life was!",
                "fr": "And so Nebamun is bragging to other elites how fantastic his life was!",
                "es": "And so Nebamun is bragging to other elites how fantastic his life was!"
            }
        }
